<form action="" class="formModal" id="newPlaceForm" name="newPlaceForm">
    <h6 class="serif">Add New Place</h6>
    <br>
    <div class="marginBottom">
        <label for="newPlaceName">Name:</label> <input id="newPlaceName" type="text" />
    </div>

    <div class="marginBottom">
        <label for="newPlaceType">Type:&nbsp;&nbsp;</label> <input style="width:250px" id="newPlaceType" type="text" />
    </div>

    <div class="marginBottom">
        <label for="isComposite">Is composite:</label> <input type="checkbox" id="isComposite" />    
    </div>

    <div class="marginBottom">
    <input type="submit" value="Submit" class="button" />
    </div>

    <div class="message"></div>
</form>
